{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Sandro",
  "license": "ISC",
  "scripts": {
    "dev": "$(npm bin)/nodemon ./src/index.js",
    "nab": "$(npm bin)/nab https://localhost:3000/api/final",
    "stress": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && $(npm bin)/nperf -c 5 -n 1000 https://localhost:3000/api/final",
    "postman": "$(npm bin)/newman run ./tests/postman/** --insecure",
    "doc": "$(npm bin)/jsdoc src -r -c ./.jsdoc.json -R ../README.md -P ./package.json -d ../docs && $(npm bin)/jsdoc2md ./src/**/** > ../DOCUMENTATION.md",
    "test": "$(npm bin)/mocha ./tests/**/**.test.js",
    "test:watch": "npm run test -- --watch",
    "coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "http-perf": "0.0.5",
    "husky": "^1.3.1",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^6.1.4",
    "newman": "^4.4.1",
    "node-ab": "0.0.6",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-express-serializer": "^1.0.0",
    "express": "^4.16.4",
    "express-request-id": "^1.4.1",
    "express-validator": "^5.3.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.2",
    "morgan": "^1.9.1",
    "spdy": "^4.0.0"
  }
}
